{% assign settingsFooterBg = settings.footer_bg %}

<footer
  class="text-white px-4 py-8 lg:px-6 lg:py-16 b-footer custom-padding"
  style="background-color: {{ settingsFooterBg }}; border-top-left-radius: {{ settings.footer_corner_radius | default: 32 }}px; border-top-right-radius: {{ settings.footer_corner_radius | default: 32 }}px;"
>
  <div class="container">
    {%- comment -%} Top row: logo + (optional) socials on the right {%- endcomment -%}
    <div class="b-footer-top flex items-start justify-between gap-6">
      <div class="flex items-center gap-4">
        {% if settings.footer_logo != blank %}
          <a href="{{ routes.root_url }}" class="inline-block">
            <img
              src="{{ settings.footer_logo | image_url: width: 320 }}"
              alt="{{ shop.name }}"
              class="block h-auto"
              style="width: {{ settings.footer_logo_width | default: 120 }}px;"
              loading="lazy"
            >
          </a>
        {% else %}
          <span class="text-xl font-semibold tracking-wide">{{ shop.name }}</span>
        {% endif %}
      </div>

      {%- comment -%} Small social icons (optional) {%- endcomment -%}
      <div class="hidden md:flex items-center gap-4">
        {% if settings.social_facebook and settings.social_facebook_icon %}
          <a href="{{ settings.social_facebook }}" target="_blank" class="opacity-90 hover:opacity-100 transition">
            <img src="{{ settings.social_facebook_icon | image_url: width: 64 }}" alt="Facebook" class="w-12 h-12">
          </a>
        {% endif %}
        {% if settings.social_instagram and settings.social_instagram_icon %}
          <a href="{{ settings.social_instagram }}" target="_blank" class="opacity-90 hover:opacity-100 transition">
            <img src="{{ settings.social_instagram_icon | image_url: width: 64 }}" alt="Instagram" class="w-12 h-12">
          </a>
        {% endif %}
        {% if settings.social_twitter and settings.social_twitter_icon %}
          <a href="{{ settings.social_twitter }}" target="_blank" class="opacity-90 hover:opacity-100 transition">
            <img src="{{ settings.social_twitter_icon | image_url: width: 64 }}" alt="Twitter/X" class="w-12 h-12">
          </a>
        {% endif %}
        {% if settings.social_youtube and settings.social_youtube_icon %}
          <a href="{{ settings.social_youtube }}" target="_blank" class="opacity-90 hover:opacity-100 transition">
            <img src="{{ settings.social_youtube_icon | image_url: width: 64 }}" alt="YouTube" class="w-6 h-6">
          </a>
        {% endif %}
      </div>
    </div>

    {%- comment -%} Middle: newsletter (left) + link columns (right) {%- endcomment -%}
    <div class="mt-16 grid gap-20 lg:gap-24 lg:grid-cols-12 items-start">
      <div class="lg:col-span-5 footer-space">
        {% if settings.newsletter_title != blank %}
          <p class="b-footer__newsletter-title mb-10 max-w-xxl">{{ settings.newsletter_title }}</p>
        {% else %}
          <p class="b-footer__newsletter-title mb-10 max-w-xxl">Don't miss out on new products, exclusive offers and much more by joining the Yuum newsletter</p>
        {% endif %}

        {% form 'customer', class: 'flex flex-col sm:flex-row items-start gap-6 max-w-xxl' %}
          <div class="relative w-full">
            <input
              type="email"
              name="contact[email]"
              class="b-footer-input peer w-full bg-transparent border-0 border-b border-white/60 focus:border-white outline-none placeholder:text-white/50 py-3"
              placeholder="{{ settings.newsletter_placeholder }}"
              required
            >
          </div>
          <button type="submit" class="btn btn-light !font-normal shrink-0 inline-flex items-center justify-center rounded-full px-8 py-3 border border-white/80 bg-white text-black hover:opacity-90 transition min-w-[200px]">
            {{ settings.newsletter_button_text }}
          </button>
        {% endform %}
      </div>

      <div class="lg:col-span-7 grid grid-cols-2 md:grid-cols-4 gap-12">
        {%- comment -%} CATALOGUE {%- endcomment -%}
        <div>
          {% if settings.footer_menu_title_1 != blank %}
            <h3 class="b-footer__title mb-8">{{ settings.footer_menu_title_1 }}</h3>
          {% endif %}
          {% for link in settings.footer_menu_1.links %}
            <div class="mb-4 last:mb-0">
              <a href="{{ link.url }}" class="b-footer__link">{{ link.title }}</a>
            </div>
          {% endfor %}
        </div>

        {%- comment -%} COMPANY {%- endcomment -%}
        <div>
          {% if settings.footer_menu_title_2 != blank %}
            <h3 class="b-footer__title mb-8">{{ settings.footer_menu_title_2 }}</h3>
          {% endif %}
          {% for link in settings.footer_menu_2.links %}
            <div class="mb-4 last:mb-0">
              <a href="{{ link.url }}" class="b-footer__link">{{ link.title }}</a>
            </div>
          {% endfor %}
        </div>

        {%- comment -%} INFORMATION {%- endcomment -%}
        <div>
          {% if settings.footer_menu_title_3 != blank %}
            <h3 class="b-footer__title mb-8">{{ settings.footer_menu_title_3 }}</h3>
          {% endif %}
          {% for link in settings.footer_menu_3.links %}
            <div class="mb-4 last:mb-0">
              <a href="{{ link.url }}" class="b-footer__link">{{ link.title }}</a>
            </div>
          {% endfor %}
        </div>

        {%- comment -%} CONTACT US {%- endcomment -%}
        <div>
          <h3 class="b-footer__title mb-8">CONTACT US</h3>
          {% if settings.contact_phone != blank %}
            <div class="mb-4 last:mb-0">
              <a href="tel:{{ settings.contact_phone | remove: ' ' }}" class="b-footer__link">{{ settings.contact_phone }}</a>
            </div>
          {% endif %}
          {% if settings.contact_email != blank %}
            <div class="mb-4 last:mb-0">
              <a href="mailto:{{ settings.contact_email }}" class="b-footer__link">{{ settings.contact_email }}</a>
            </div>
          {% endif %}
          <div class="mb-4 last:mb-0">
            <a href="#" class="b-footer__link">WhatsApp</a>
          </div>
        </div>
      </div>
    </div>

    {%- comment -%} Big tagline row {%- endcomment -%}
    {% if settings.footer_tagline_image != blank %}
      <div class="mt-20 lg:mt-24 flex justify-center footer-space-l">
        <img
          src="{{ settings.footer_tagline_image | image_url: width: 1600 }}"
          alt="{{ settings.footer_tagline | default: shop.name }}"
          class="max-w-full h-auto"
          style="max-height: {{ settings.footer_tagline_height | default: 240 }}px;"
          loading="lazy"
        >
      </div>
    {% elsif settings.footer_tagline != blank %}
      <div class="mt-20 lg:mt-24">
        <div class="leading-none tracking-tight opacity-20 select-none text-center"
             style="font-size: {{ settings.footer_tagline_size | default: 128 }}px; line-height: 0.9;">
          {{ settings.footer_tagline }}
        </div>
      </div>
    {% endif %}

    {%- comment -%} Bottom bar {%- endcomment -%}
    {%- comment -%} Bottom bar {%- endcomment -%}
    <div class="mt-[40px] border-t pt-[24px] pb-[24px] border-white/20 flex flex-col-reverse md:flex-row items-center justify-between gap-6">

      <div class="flex items-center gap-8">
        <div class="b-footer__copyright">
          {{ settings.copyright_text }}
        </div>
        {% if settings.footer_menu_4 and settings.footer_menu_4.links.size > 0 %}
          {% for link in settings.footer_menu_4.links %}
            <a href="{{ link.url }}" class="b-footer__bottom-link">{{ link.title }}</a>
          {% endfor %}
        {% endif %}
      </div>

