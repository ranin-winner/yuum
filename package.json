{
  "name": "theme-starter",
  "version": "1.0.3",
  "type": "module",
  "private": true,
  "scripts": {
    "clear": "rimraf node_modules/.vite",
    "dev": "run-p -sr \"shopify:dev -- {@}\" \"vite:dev\" --",
