    "@shopify/theme": "^3.58.2",
    "autoprefixer": "^10.4.20",
    "bumpp": "^9.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "sass": "^1.80.6",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.1",
    "vite-plugin-shopify": "^3.0.1"
  },
  "dependencies": {
    "@alpinejs/persist": "^3.14.3",
    "alpinejs": "^3.14.3",
    "swiper": "^11.1.15"
