{
  "name": "theme-starter",
  "version": "1.0.3",
  "type": "module",
  "private": true,
  "scripts": {
    "clear": "rimraf node_modules/.vite .vite assets/.vite dist",

    "predev": "shopify theme dev --store yuumuk.myshopify.com",
    "shopify:dev": "shopify theme dev --store yuumuk.myshopify.com --host 127.0.0.1 --port 9292 --no-update",
    "vite:dev": "vite",
    "dev": "npm-run-all predev -p \"shopify:dev\" \"vite:dev\"",

    "build": "vite build",
    "dev:built": "npm run build && shopify theme dev --store yuumuk.myshopify.com --host 127.0.0.1 --port 9292 --no-update",

    "check": "shopify theme check",
    "pull": "shopify theme pull -d -o sections/*.json templates/*.json templates/customers/*.json config/settings_data.json",
    "release": "bumpp"
  },
  "devDependencies": {
    "@by-association-only/vite-plugin-shopify-clean": "^2.0.0",
    "@shopify/cli": "^3.65.3",
    "@shopify/prettier-plugin-liquid": "^1.5.0",
    "@shopify/theme": "^3.58.2",
    "autoprefixer": "^10.4.20",
    "bumpp": "^9.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "sass": "^1.80.6",
    "tailwindcss": "^3.4.10",
    "vite": "^7.2.0",
    "vite-plugin-shopify": "^4.0.2"
  },
  "dependencies": {
    "@alpinejs/persist": "^3.14.3",
    "@studio-freight/lenis": "^1.0.42",
    "alpinejs": "^3.14.3",
    "gsap": "^3.13.0",
    "swiper": "^11.1.15"
  }
}
